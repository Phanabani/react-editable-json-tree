{
  "root": true,
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/extensions": [2],
    "jsx-a11y/no-static-element-interactions": [1],
    "no-use-before-define": [0],
    "react/jsx-filename-extension": [0]
  }
}
