{
  "name": "react-editable-json-tree",
  "version": "2.3.0",
  "description": "React Editable Json Tree",
  "source": "src/JsonTree.js",
  "main": "dist/JsonTree.cjs.js",
  "module": "dist/JsonTree.esm.js",
  "types": "dist/JsonTree.d.ts",
  "targets": {
    "types": false
  },
  "scripts": {
    "build": "parcel build",
    "prepublishOnly": "yarn run build",
